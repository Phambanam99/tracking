# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/tracking?schema=public"
DIRECT_DATABASE_URL="postgresql://username:password@localhost:5432/tracking?schema=public"

# Redis Configuration
REDIS_URL="redis://localhost:6379"

# Server Configuration
PORT=3001
NODE_ENV=development
FRONTEND_ORIGIN=http://localhost:4000

# AIS Configuration
AIS_AUTO_TRIGGER=true
AIS_AUTO_TRIGGER_INTERVAL_MS=60000
AIS_QUERY_MINUTES=60

# AI Stream API
AI_STREAM_API="your-aistream-api-key-here"
AISTREAM_ENABLED=true
AISTREAM_ENDPOINT='wss://stream.aisstream.io/v0/stream'
AISTREAM_POLL_INTERVAL_MS=30000
VESSEL_ENRICHMENT_ENABLED=true

# Vessel Enrichment - MarineTraffic Access Token (optional)
# Get your token from: https://www.marinetraffic.com (login required)
# Without token: limited to public data only
MARINETRAFFIC_ACCESS_TOKEN="your-marinetraffic-jwt-token-here"

# JWT Configuration
# IMPORTANT: Generate a secure secret using: openssl rand -base64 32
JWT_SECRET=your-secure-jwt-secret-minimum-32-characters-here

# Weather API Configuration
# Get your API key from: https://openweathermap.org/api
OPENWEATHER_API_KEY=your-openweathermap-api-key-here

# Logging
LOG_LEVEL=warn

# Node.js Memory Configuration
NODE_OPTIONS=--max-old-space-size=4096

# ADSB Configuration
ADSB_COLLECTOR_INTERVAL=30
ADSB_USE_SIMULATED_DATA=false
ADSB_COLLECTOR_ENABLED=true
ADSB_EXTERNAL_API_URL=http://your-adsb-server:6001/api/osint
ADSB_JOB_TIMEOUT=30000
ADSB_MAX_CONCURRENT_BATCHES=5
ADSB_QUEUE_MAX_JOBS_PER_SECOND=10
ADSB_QUEUE_RETRY_ATTEMPTS=3
ADSB_QUEUE_RETRY_DELAY=5000

# Database Pool Configuration
# Add your database pool settings here if needed
